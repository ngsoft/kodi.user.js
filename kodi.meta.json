{
    "name": "KodiCast",
    "description": "Cast your streams to Kodi",
    "author": "Aymeric Anger",
    "namespace": "https://github.com/ngsoft/kodi.user.js",
    "run-at": "document-end",
    "noframes": true,
    "grant": [
        "GM_setValue",
        "GM_getValue",
        "GM_deleteValue",
        "GM_listValues",
        "GM_addValueChangeListener",
        "GM_removeValueChangeListener",
        "GM_notification",
        "GM_xmlhttpRequest",
        "GM_registerMenuCommand",
        "GM_unregisterMenuCommand",
        "GM_getResourceText",
        "GM_addStyle",
        "GM_setClipboard"
    ],
    "resource": {
        "iziToastCSS": "https://cdn.jsdelivr.net/npm/izitoast/dist/css/iziToast.css"
    },
    "require": [
        "https://cdn.jsdelivr.net/gh/mathiasbynens/utf8.js/utf8.min.js",
        "https://cdn.jsdelivr.net/npm/izitoast/dist/js/iziToast.min.js",
        "https://cdn.jsdelivr.net/gh/mitchellmebane/GM_fetch@master/GM_fetch.js",
        "/lib/require.js"
    ],
    "basepath": "https://cdn.jsdelivr.net/gh/ngsoft/kodi.user.js@",
    "include": [
        "*"
    ],
    "connect": [
        "*"
    ],
    "defaulticon": {
        "url": "data:image/png;base64,",
        "base64URL": "data:image/png;base64,"
    }
}