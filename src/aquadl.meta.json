{
    "name": "AquaManga Downloader",
    "description": "Download on AquaManga",
    "author": "Aymeric Anger",
    "namespace": "https://github.com/ngsoft/kodi.user.js",
    "noframes": true,
    "grant": [
        "GM_registerMenuCommand",
        "GM_unregisterMenuCommand",
        "GM_getResourceURL",
        "GM_xmlhttpRequest"
    ],
    "resource": {
        "overlay": "/lib/styles/gmoverlay.css",
        "asurastyle": "/lib/styles/asurastyle.css",
        "asuraui": "/lib/templates/asuraui.html",
        "minidark": "https://cdnjs.cloudflare.com/ajax/libs/mini.css/3.0.1/mini-dark.min.css",
        "minilight": "https://cdnjs.cloudflare.com/ajax/libs/mini.css/3.0.1/mini-default.min.css",
        "btstutils": "https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap-utilities.min.css"
    },
    "require": [
        "https://cdn.jsdelivr.net/npm/pdf-lib@latest/dist/pdf-lib.min.js",
        "https://cdn.jsdelivr.net/npm/jszip@3.1.5/dist/jszip.min.js"
    ],
    "module": [
        "asuraui",
        "cpromise"
    ],
    "include": [

    ],
    "match": [
        "*://aquamanga.com/read/*"
    ],
    "connect": [
        "*"
    ],
    "icon": {
        "url": "data:image/png;base64,"

    }
}