{
    "name": "Manga Downloader",
    "description": "Download Mangas",
    "author": "Aymeric Anger",
    "namespace": "https://github.com/ngsoft/kodi.user.js",
    "noframes": true,
    "grant": [
        "GM_xmlhttpRequest",
        "GM_registerMenuCommand",
        "GM_unregisterMenuCommand",
        "GM_addStyle"
    ],
    "resource": [],
    "require": [
        "https://cdn.jsdelivr.net/npm/pdf-lib@latest/dist/pdf-lib.min.js"
    ],
    "module": [
        "gmfetch",
        "emitter",
        "gmtools",
        "finder"
    ],
    "include": [
        "/^https?:\\/\\/beta\\.asurascans\\.\\w+\\/(comics|read)\\//",
        "/^https?:\\/\\/www\\.asurascans\\.\\w+\\/.*\\//"
    ],
    "connect": [
        "*"
    ],
    "icon": {
        "url": "https://www.asurascans.com/wp-content/uploads/2021/03/cropped-Group_1-1-32x32.png",
        "base64URL": "data:image/png;base64,"
    }
}